import java.util.Scanner;
public class Swapping {
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the two numbers:");
        int num1=sc.nextInt();
        int num2=sc.nextInt();
        System.out.println("Before Swapping:\nnum1="+num1+"\n"+"num2="+num2);
        num1=num1+num2;
        num2=num1-num2;
        num1=num1-num2;
        System.out.println("After Swapping:\nnum1="+num1+"\n"+"num2="+num2);
    }
}
